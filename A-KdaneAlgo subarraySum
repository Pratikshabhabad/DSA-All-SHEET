//  firsst kdanes algorithm
class Solution{

    // arr: input array
    // n: size of array
    //Function to find the sum of contiguous subarray with maximum sum.
    long maxSubarraySum(int arr[], int n){
        
        // Your code here
        int Max=Integer.MIN_VALUE;
        for(int i=0;i<n;i++)
        {
            int currentSum=0;
            for(int j=i;j<n;j++)
            {
                currentSum+=arr[j];
                 Max=Math.max(currentSum,Max);
            }
          
        }
        
        return Max;
        
    }
    
}
